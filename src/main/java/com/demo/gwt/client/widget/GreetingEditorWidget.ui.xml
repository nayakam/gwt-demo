<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.demo.gwt.client.widget" xmlns:widget="urn:import:com.demo.gwt.client.widget"
             xmlns:widget2="urn:import:com.demo.gwt.client.widget">
    <ui:style>
    </ui:style>
    <g:HTMLPanel>
        <div>
            <div>
                <label>Phrase:</label>
                <g:TextBox ui:field="phraseTextBox"/>
            </div>
            <div>
                <label>Title:</label>
                <g:TextBox ui:field="titleTextBox"/>
            </div>
            <div>
                <label>Name:</label>
                <g:TextBox ui:field="nameTextBox"/>
            </div>
            <div>
                <label>Repeat:</label>
                <g:IntegerBox ui:field="repeatIntegerBox"/>
            </div>
            <div>
                <g:Button ui:field="okButton" text="Ok"/>
                <g:Button ui:field="serverButton" text="Call server" />
            </div>
        </div>
        <hr/>
        <widget2:GreetingWidget ui:field="greetingWidget"/>
    </g:HTMLPanel>
</ui:UiBinder>